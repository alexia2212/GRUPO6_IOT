<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:right="1dp" android:left="1dp" android:top="1dp" android:bottom="1dp">
        <shape android:shape="rectangle">
            <solid android:color="#FFFFFF" /> <!-- Color de fondo blanco -->
            <stroke
                android:width="1dp"
            android:color="#FFFFFF" /> <!-- Color del borde -->
            <corners
                android:radius="20dp"
                android:color="#FFFFFF"/> <!-- Radio de las esquinas (ajústalo según lo desees) -->
        </shape>
    </item>
</layer-list>