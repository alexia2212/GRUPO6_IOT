<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondo"
    tools:context=".Registro">

    <Button
        android:id="@+id/botonEgresado"
        android:layout_width="179dp"
        android:layout_height="42dp"
        android:backgroundTint="@color/material_dynamic_neutral40"
        android:text="Egresado"
        android:textColor="@color/white"
        tools:layout_editor_absoluteX="124dp"
        tools:layout_editor_absoluteY="722dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="206dp"
        android:layout_height="180dp"
        app:srcCompat="@drawable/logo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0"
         />

    <TextView
        android:id="@+id/bienvenidos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="¡Bienvenido a Tech-Bat!"
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:textSize="22dp"
        android:textStyle="bold"
        app:layout_constraintVertical_bias="0.165" />

    <EditText
        android:id="@+id/editTextPhone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="phone"
        tools:layout_editor_absoluteX="97dp"
        tools:layout_editor_absoluteY="365dp" />

    <EditText
        android:id="@+id/nombres"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="Nombres"
        tools:layout_editor_absoluteX="96dp"
        tools:layout_editor_absoluteY="258dp" />

    <Button
        android:id="@+id/guardar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Guardar Registro"
        tools:layout_editor_absoluteX="132dp"
        tools:layout_editor_absoluteY="856dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Acepto Terminos y Condiciones"
        tools:layout_editor_absoluteX="108dp"
        tools:layout_editor_absoluteY="820dp" />

    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="314dp"
        tools:layout_editor_absoluteY="808dp" />

    <EditText
        android:id="@+id/editTextTextPassword2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="104dp"
        tools:layout_editor_absoluteY="552dp" />

    <EditText
        android:id="@+id/apellidos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="Apellidos"
        tools:layout_editor_absoluteX="97dp"
        tools:layout_editor_absoluteY="310dp" />

    <EditText
        android:id="@+id/editTextTextEmailAddress2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textEmailAddress"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="426dp" />

    <EditText
        android:id="@+id/editTextTextPassword"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="492dp" />

    <Button
        android:id="@+id/iniciosesion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Iniciar Sesión"
        tools:layout_editor_absoluteX="108dp"
        tools:layout_editor_absoluteY="204dp" />

    <Button
        android:id="@+id/register"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Registrarse"
        tools:layout_editor_absoluteX="210dp"
        tools:layout_editor_absoluteY="203dp" />

    <Button
        android:id="@+id/condicionUsuario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Condición de Usuario"
        android:textColor="@color/white"
        android:backgroundTint="@color/material_dynamic_neutral40"
        tools:layout_editor_absoluteX="122dp"
        tools:layout_editor_absoluteY="626dp" />

    <Button
        android:id="@+id/botonAlumno"
        android:layout_width="182dp"
        android:layout_height="36dp"
        android:text="Alumno"
        android:textColor="@color/white"
        android:backgroundTint="@color/material_dynamic_neutral40"
        tools:layout_editor_absoluteX="123dp"
        tools:layout_editor_absoluteY="674dp" />

    <View
        android:id="@+id/view5"
        android:layout_width="442dp"
        android:layout_height="803dp"
        android:background="@color/white"
        android:elevation="-10dp"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="161dp" />

</androidx.constraintlayout.widget.ConstraintLayout>