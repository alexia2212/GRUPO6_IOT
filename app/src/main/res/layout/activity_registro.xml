<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondo"
    tools:context=".Registro">

    <Button
        android:id="@+id/botonEgresado"
        android:layout_width="257dp"
        android:layout_height="39dp"
        android:backgroundTint="@color/material_dynamic_neutral40"
        android:text="Egresado"
        android:textColor="@color/white"
        app:layout_constraintTop_toTopOf="@+id/view5"
        app:layout_constraintBottom_toBottomOf="@+id/view5"
        app:layout_constraintStart_toStartOf="@+id/botonAlumno"
        app:layout_constraintEnd_toEndOf="@+id/botonAlumno"
        app:layout_constraintVertical_bias="0.8"
        app:layout_constraintHorizontal_bias="0.5" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="191dp"
        android:layout_height="166dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/logo" />

    <TextView
        android:id="@+id/bienvenidos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="¡Bienvenido a Tech-Bat!"
        android:textColor="@color/black"
        android:elevation="18dp"
        android:textSize="25dp"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="@+id/view5"
        app:layout_constraintBottom_toBottomOf="@+id/view5"
        app:layout_constraintStart_toStartOf="@+id/view5"
        app:layout_constraintEnd_toEndOf="@+id/view5"
        app:layout_constraintVertical_bias="0.03"
        app:layout_constraintHorizontal_bias="0.5" />


    <EditText
        android:id="@+id/nombres"
        android:layout_width="257dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="Nombres"
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="@+id/view5"
        app:layout_constraintBottom_toBottomOf="@+id/view5"
        app:layout_constraintStart_toStartOf="@+id/view5"
        app:layout_constraintEnd_toEndOf="@+id/view5"
        app:layout_constraintVertical_bias="0.25"
        app:layout_constraintHorizontal_bias="0.5" />

    <Button
        android:id="@+id/guardar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Guardar Registro"
        android:textColor="@color/white"
        android:textSize="16dp"
        android:backgroundTint="@color/material_dynamic_secondary10"
        app:layout_constraintTop_toTopOf="@+id/view5"
        app:layout_constraintBottom_toBottomOf="@+id/view5"
        app:layout_constraintStart_toStartOf="@+id/view5"
        app:layout_constraintEnd_toEndOf="@+id/view5"
        app:layout_constraintVertical_bias="0.965"
        app:layout_constraintHorizontal_bias="0.5" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Acepto Terminos y Condiciones"
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="@+id/view5"
        app:layout_constraintBottom_toBottomOf="@+id/view5"
        app:layout_constraintStart_toStartOf="@+id/view5"
        app:layout_constraintEnd_toEndOf="@+id/view5"
        app:layout_constraintVertical_bias="0.87"
        app:layout_constraintHorizontal_bias="0.4" />

    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@+id/view5"
        app:layout_constraintBottom_toBottomOf="@+id/view5"
        app:layout_constraintStart_toStartOf="@+id/view5"
        app:layout_constraintEnd_toEndOf="@+id/view5"
        app:layout_constraintVertical_bias="0.8742"
        app:layout_constraintHorizontal_bias="0.77" />



    <EditText
        android:id="@+id/apellidos"
        android:layout_width="257dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="Apellidos"
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="@+id/view5"
        app:layout_constraintBottom_toBottomOf="@+id/view5"
        app:layout_constraintStart_toStartOf="@+id/nombres"
        app:layout_constraintEnd_toEndOf="@+id/nombres"
        app:layout_constraintVertical_bias="0.32"
        app:layout_constraintHorizontal_bias="0.5" />

    <Button
        android:id="@+id/iniciosesion"
        android:layout_width="163dp"
        android:layout_height="59dp"
        android:elevation="50dp"
        android:text="Iniciar Sesión   "
        android:textSize="15dp"
        android:textColor="@color/white"
        android:backgroundTint="@color/material_dynamic_neutral70"
        app:layout_constraintBottom_toBottomOf="@+id/view5"
        app:layout_constraintEnd_toEndOf="@+id/view5"
        app:layout_constraintHorizontal_bias="0.2"
        app:layout_constraintStart_toStartOf="@+id/view5"
        app:layout_constraintTop_toTopOf="@+id/view5"
        app:layout_constraintVertical_bias="0.12" />

    <Button
        android:id="@+id/register"
        android:layout_width="163dp"
        android:layout_height="59dp"
        android:text="Registrarse"
        android:elevation="-30dp"
        android:textSize="15dp"
        android:textColor="@color/white"
        android:backgroundTint="@color/material_dynamic_primary60"
        app:layout_constraintBottom_toBottomOf="@+id/view5"
        app:layout_constraintEnd_toEndOf="@+id/view5"
        app:layout_constraintHorizontal_bias="0.85"
        app:layout_constraintStart_toStartOf="@+id/view5"
        app:layout_constraintTop_toTopOf="@+id/view5"
        app:layout_constraintVertical_bias="0.12" />

    <Button
        android:id="@+id/condicionUsuario"
        android:layout_width="257dp"
        android:layout_height="45dp"
        android:text="Condición de Usuario"
        android:backgroundTint="@color/gris"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="@+id/view5"
        app:layout_constraintEnd_toEndOf="@+id/view5"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/view5"
        app:layout_constraintTop_toTopOf="@+id/view5"
        app:layout_constraintVertical_bias="0.7" />

    <Button
        android:id="@+id/botonAlumno"
        android:layout_width="257dp"
        android:layout_height="39dp"
        android:text="Alumno"
        android:backgroundTint="@color/material_dynamic_neutral40"
        android:textColor="@color/white"
        app:layout_constraintTop_toTopOf="@+id/view5"
        app:layout_constraintBottom_toBottomOf="@+id/view5"
        app:layout_constraintStart_toStartOf="@+id/condicionUsuario"
        app:layout_constraintEnd_toEndOf="@+id/condicionUsuario"
        app:layout_constraintVertical_bias="0.75"
        app:layout_constraintHorizontal_bias="0.5" />

    <View
        android:id="@+id/view5"
        android:layout_width="352dp"
        android:layout_height="557dp"
        android:alpha="0.8"
        android:background="@drawable/background7"
        android:elevation="-10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.848" />



    <EditText
        android:id="@+id/editTextCorreo"
        android:layout_width="257dp"
        android:layout_height="45dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        app:layout_constraintTop_toTopOf="@+id/view5"
        app:layout_constraintBottom_toBottomOf="@+id/view5"
        app:layout_constraintStart_toStartOf="@+id/editCodigoPucp"
        app:layout_constraintEnd_toEndOf="@+id/editCodigoPucp"
        app:layout_constraintVertical_bias="0.42"
        app:layout_constraintHorizontal_bias="0.5" />

    <EditText
        android:id="@+id/editTextPassword"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintTop_toTopOf="@+id/view5"
        app:layout_constraintBottom_toBottomOf="@+id/view5"
        app:layout_constraintStart_toStartOf="@+id/editTextCorreo"
        app:layout_constraintEnd_toEndOf="@+id/editTextCorreo"
        app:layout_constraintVertical_bias="0.49"
        app:layout_constraintHorizontal_bias="0.5" />

    <EditText
        android:id="@+id/editRepetirPassword"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintTop_toTopOf="@+id/view5"
        app:layout_constraintBottom_toBottomOf="@+id/view5"
        app:layout_constraintStart_toStartOf="@+id/editTextPassword"
        app:layout_constraintEnd_toEndOf="@+id/editTextPassword"
        app:layout_constraintVertical_bias="0.56"
        app:layout_constraintHorizontal_bias="0.5" />

    <EditText
        android:id="@+id/editCodigoPucp"
        android:layout_width="257dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintTop_toTopOf="@+id/view5"
        app:layout_constraintBottom_toBottomOf="@+id/view5"
        app:layout_constraintStart_toStartOf="@+id/apellidos"
        app:layout_constraintEnd_toEndOf="@+id/apellidos"
        app:layout_constraintVertical_bias="0.35"
        app:layout_constraintHorizontal_bias="0.5"/>

</androidx.constraintlayout.widget.ConstraintLayout>